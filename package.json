{
  "name": "react-training",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start-development": "npm run build:backend:development && npm run build:frontend:development && node dist/server.js",
    "start-production": "npm run build:backend:production && npm run build:frontend:production && node dist/server.js",
    "build:frontend:production": "webpack --mode production --env.production --config=./webpack.frontend.js",
    "build:backend:production": "webpack --mode production --env.production --config=./webpack.backend.js",
    "build:frontend:development": "webpack --mode development --env.development --config=./webpack.frontend.js",
    "build:backend:development": "webpack --mode development --env.development --config=./webpack.backend.js",
    "check-style": "eslint --fix webpack.backend.js webpack.frontend.js ./src/**/*.tsx ./src/**/*.ts",
    "test": "jest",
    "test-update": "jest --updateSnapshot"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/test-shim.js",
      "<rootDir>/test-setup.js"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleNameMapper": {
      "^.+\\.(css|less|scss|sass)$": "identity-obj-proxy"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?|ts)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "coverageDirectory": "./dist/cov",
    "collectCoverage": true,
    "coverageReporters": [
      "json",
      "html"
    ],
    "verbose": true
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.17.1",
    "react-dom": "^16.11.0",
    "react-redux": "^7.1.3",
    "redux": "^4.0.4",
    "redux-router": "^2.1.2",
    "sass-loader": "^8.0.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-es2015": "^7.0.0-beta.53",
    "@babel/preset-react": "^7.7.0",
    "@babel/preset-typescript": "^7.7.0",
    "@babel/register": "^7.7.0",
    "@babel/runtime": "^7.7.1",
    "@testing-library/react": "^9.3.2",
    "@types/enzyme": "^3.10.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/express": "^4.17.2",
    "@types/jest": "^24.0.23",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.3",
    "@types/react-redux": "^7.1.5",
    "@types/react-test-renderer": "^16.9.1",
    "@typescript-eslint/eslint-plugin": "^2.6.1",
    "@typescript-eslint/parser": "^2.6.1",
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "identity-obj-proxy": "^3.0.0",
    "isomorphic-style-loader": "^5.1.0",
    "jest": "^24.9.0",
    "jest-cli": "^24.9.0",
    "json-loader": "^0.5.7",
    "node-sass": "^4.13.0",
    "prettier": "^1.18.2",
    "react": "^16.11.0",
    "react-test-renderer": "^16.11.0",
    "snazzy": "^8.0.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.0.0",
    "terser-webpack-plugin": "^2.2.1",
    "ts-jest": "^24.1.0",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.2",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-node-externals": "^1.7.2"
  }
}
